<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="imageBox" alignment="CENTER" onDragDropped="#dragDropped" onDragEntered="#dragEnter" onDragExited="#dragExit" onDragOver="#dragOver" prefHeight="200.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.vision.control.DragDropController">
    <children>
        <ImageView fx:id="previousImageView" fitHeight="300.0" pickOnBounds="true" preserveRatio="true" smooth="false" />
        <Label fx:id="label" text="%key1" />
        <ImageView fx:id="processedImageView" fitHeight="300.0" pickOnBounds="true" preserveRatio="true" smooth="false" />
        <CheckBox fx:id="mapBox" disable="true" mnemonicParsing="false" text="Map" />
    </children>
</VBox>
